<beans xmlns="http://www.springframework.org/schema/beans" xmlns:p="http://www.springframework.org/schema/p"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="
        http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"
	   default-autowire="no" default-lazy-init="true">

	<!-- fast dfs 链接对象池 -->
	<bean id="fastDFSSource" class="cn.eakay.demo.biz.common.fastdfspool.BasicFastDFSSource" destroy-method="close">
		<property name="initialSize" value="${demo.fastdfspool.initialSize}" />
		<property name="maxActive" value="${demo.fastdfspool.maxActive}" />
		<property name="maxIdle" value="${demo.fastdfspool.maxIdle}" />
		<property name="maxWait" value="${demo.fastdfspool.maxWait}" />
	</bean>

</beans>